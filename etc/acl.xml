<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <!-- Menu -->
                    <resource id="MagePsycho_RegionCityPro::menu_section" title="Region &amp; City Manager" translate="title" sortOrder="100">
                        <resource id="MagePsycho_RegionCityPro::region" title="Manage Regions" translate="title" sortOrder="30" />
                        <resource id="MagePsycho_RegionCityPro::city" title="Manage Cities" translate="title" sortOrder="40" />
                        <resource id="MagePsycho_RegionCityPro::sub_district" title="Manage Sub Districts" translate="title" sortOrder="45" />
                        <resource id="MagePsycho_RegionCityPro::menu_config" title="Manage Settings" translate="title" sortOrder="50" />
                    </resource>

                    <!-- Settings -->
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="MagePsycho_RegionCityPro::config" title="Region &amp; City Manager Section" translate="title" sortOrder="50" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
